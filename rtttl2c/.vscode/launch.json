    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch Metal Gear",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/rtttl2c",
            "args": [
                "Swadgetones/FourSeasons.txt",
                "Swadgetones/MetalGear.txt",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "make"
        },
        {
            "name": "(gdb) Launch Vivaldi",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/rtttl2c",
            "args": [
                "FourSeasons:d=16,o=5,b=105:8e,32g#,32g#,32g#,32p,32g#,32g#,32g#,32p,32g#,32g#,32g#,32p,f#,e,4b.,32p,b,a,32g#,32g#,32g#,32p,32g#,32g#,32g#,32p,32g#,32g#,32g#,32p,f#,e,4b.,32p,b,a,32g#,32g#,32g#,32p,a,b,8a,32g#,32g#,32g#,32p,8f#,8d#,4b4.,8e,32g#,32g#,32g#,32p,32g#,32g#,32g#,32p,32g#,32g#,32g#,32p,f#,e,4b.,32p,b,a,32g#,32g#,32g#,32p,32g#,32g#,32g#,32p,32g#,32g#,32g#,32p,f#,e,4b.,32p,b,a,32g#,32g#,32g#,32p,a,b,8a,32g#,32g#,32g#,32p,8f#,8d#,4b4."
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}